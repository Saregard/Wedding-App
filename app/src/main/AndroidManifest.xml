<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.weddingapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WeddingApp">
        <activity
            android:name=".DataInput"
            android:exported="false" />
        <activity android:name=".KyrkaScreen" />
        <activity android:name=".LokalScreen" />
        <activity android:name=".QuestionsScreen" />
        <activity android:name=".BordsplaceringScreen" />
        <activity android:name=".KladkodScreen" />
        <activity android:name=".WishlistScreen" />
        <activity android:name=".KorschemaScreen" />
        <activity android:name=".PlatsScreen" />
        <activity android:name=".BoendeScreen" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>